    Private Sub btnLore_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnLore.Click
        MsgBox("The surveyors return'ed fromme the perilous quest to survee the unknowne lannd havinng mani gigabittes of datar.")
        MsgBox("Thus saith the surveyors, ""Howeso shaln we procces such manner of datar?  Therein be mani gigabittes of datar!  No hummane can handel such emmensities!""")
        MsgBox("Butte suddennly fromme the void came forth a WORDSURV!")
        MsgBox("Thus saith the surveyors, ""Behold, a devisse most able at taskes of borredom and precission, and able to finish most quikly!""")
        MsgBox("Continuud the surveyors, ""Behold, what taskes which in formeur dayes commandened a multitude of fortnigts to process by mannere of pensille and parchmente mayght now require mere dayes!""")
        MsgBox("As the tymes goeth onwaerd, the surveyors assembeld sevrel MEGS of stuffs.")
        MsgBox("Thus saith the surveyors, ""Wherein shaln we finden a program of sufficient powre and speade to processe thise many MEGS?""")
        MsgBox("One saith thise, anothre saith that.")
        MsgBox("When sudenly fromme the void flummoxed forth a WORDSURV!")
        MsgBox("Quoth the surveyors, ""Behold what mannre of WORDSURV this be?  It looketh not as the DOS.""")
        If DoLog Then Log.Add("Viewed the WordSurv Lore")
    End Sub



